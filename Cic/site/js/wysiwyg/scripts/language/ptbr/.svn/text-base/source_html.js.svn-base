function loadTxt()
    {
    document.getElementById("txtLang").innerHTML = "Ajustar Texto";
    document.getElementById("btnCancel").value = "cancelar";
    document.getElementById("btnApply").value = "aplicar";
    document.getElementById("btnOk").value = " ok ";
    }
function getTxt(s)
    {
    switch(s)
        {
        case "Search":return "Localizar";
        case "Cut":return "Recortar";
        case "Copy":return "Copiar";
        case "Paste":return "Colar";
        case "Undo":return "Desfazer";
        case "Redo":return "Refazer";
        default:return "";
        }
    }
function writeTitle()
    {
    document.write("<title>Código Fonte</title>")
    }
